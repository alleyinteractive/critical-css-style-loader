{
  "name": "critical-style-loader",
  "version": "2.0.0-beta.1",
  "description": "A webpack loader for rendering critical path CSS on the server with utilities to avoid client side style-loader style duplications.",
  "keywords": [
    "webpack",
    "style-loader",
    "react",
    "isomorphic",
    "universal",
    "critical css"
  ],
  "main": "lib/loader.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --color src",
    "prepare": "npm run-script build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alleyinteractive/critical-style-loader.git"
  },
  "author": "Alley (alley.co)",
  "contributors": [
    "Owen Stowe <owen@alley.co> (alley.co)",
    "Max Eddy <max@alley.co> (alley.co)"
  ],
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/alleyinteractive/critical-style-loader/issues"
  },
  "homepage": "https://github.com/alleyinteractive/critical-style-loader#readme",
  "engine": {
    "node": "8",
    "npm": "6"
  },
  "dependencies": {
    "loader-utils": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "rimraf": "^2.6.2"
  }
